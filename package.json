{
  "name": "calendar-academic",
  "version": "0.1.0",
  "scripts": {
    "android": "tns run android --bundle",
    "ios": "tns run ios --bundle",
    "ng": "ng",
    "start-client": "ng serve -o --port 3200 --host calendar-academic.fmi.unibuc.ro --proxy-config proxy.conf.json",
    "start-server": "tsc -p ./server/tsconfig.json --watch | nodemon ./dist/server/index.js --watch ./dist/server",
    "build-client": "rimraf ./dist/client && ng build --prod",
    "build-server": "rimraf ./dist/server && tsc -p ./server/tsconfig.json",
    "build": "npm run build-client && npm run build-server",
    "test": "ng test",
    "lint": "ng lint"
  },
